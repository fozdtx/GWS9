<!DOCTYPE html>
<html>

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>App Market</title>
  <link rel="stylesheet" href="app_market.css">
  
</head>

<body>

<style>
  
</style>

<div id="apps">
  
</div>
<div>
  <div id="app_logs">
    
  </div>
  <div class="app-market-top">
    App Market
  </div>
  <div style="float: left;background-color: #141414;width: 100%;height: 400px;">
    <div style="display: flex;">
  <div class="app-market-apps" onclick="app_market_install_Youtube()">
    <img src="apps/normal/YouTube.png" alt="You Tube App Market">
    <br>
    YouTube
  </div>
  <div class="app-market-apps" onclick="app_market_install_Facebook()">
    <img src="apps/normal/facebook.png" alt="Facebook App Market">
    <br>
    Facebook
  </div>
  <div class="app-market-apps" onclick="app_market_install_Gmail()">
    <img src="apps/normal/gmail.png" alt="G-Mail App Market">
    <br>
    G-mail
  </div>
<div class="app-market-apps" onclick="app_market_install_Hotstar()">
    <img src="apps/normal/hotstar.jpg" alt="Hot star App Market">
    <br>
    Hot Star
  </div>
  <div class="app-market-apps" onclick="app_market_install_Spotify()">
    <img src="apps/normal/spotify.png" alt="Spotify App Market">
    <br>
    Spotify
  </div>
  <div class="app-market-apps" onclick="app_market_install_Vscode()">
    <img src="apps/normal/vscode.png" alt="Vs Code App Market">
    <br>
    Vs Code
  </div>
  <div class="app-market-apps" onclick="app_market_install_HMG()">
    <img src="apps/normal/hacker simulator mg beta.jpg" alt="Hacker Simulator Mg Beta App Market">
    <br>
    HMG
  </div>


 
  <div style="float: right;color: white;margin-left: 70%;border-left: 2px solid white;height: 400px; padding-left: 45px;">
  <h1>
    menu
  </h1><br>
  <hr>
  <br>
  App Name : <b id="app_name">Not selected</b>
</div>
</div>
<script>
function app_market_install_Youtube(){
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="youtube()"><img src="apps/normal/YouTube.png"><br>You Tube </div>'
}

function app_market_install_Facebook() {
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="facebook()"><img src="apps/normal/facebook.png"><br>Facebook </div>'
}

function app_market_install_Gmail() {
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="gmail()"><img src="apps/normal/gmail.png"><br>G-Mail </div>'
}


function app_market_install_Hotstar() {
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="hotstar()"><img src="apps/normal/hotstar.jpg"><br>Hot Star </div>'
}



function app_market_install_Spotify() {
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="spotify()"><img src="apps/normal/spotify.png"><br>Spotify </div>'
}

function app_market_install_Vscode() {
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="vscode()"><img src="apps/normal/vscode.png"><br>Vs Code </div>'
}


function app_market_install_HMG() {
  document.getElementById("apps").innerHTML += '<div class="apps" onclick="hmg()"><img src="apps/normal/hacker simulator mg beta.jpg"><br>HMG... </div>'
}

</script>




</body>

</html>
































